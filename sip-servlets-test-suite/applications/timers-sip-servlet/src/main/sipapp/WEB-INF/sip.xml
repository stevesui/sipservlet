<?xml version="1.0" encoding="UTF-8"?>

<sip-app>
	<app-name>org.mobicents.servlet.sip.testsuite.TimersApplication</app-name>
	<display-name>Timers Test Sip Servlet</display-name>
	<description>Timers Test Sip Servlet</description>

	<servlet-selection>	
		<main-servlet>
			TimersSipServlet
		</main-servlet>
	</servlet-selection>

	<servlet>
		<servlet-name>TimersSipServlet</servlet-name>
		<display-name>TimersSipServlet</display-name>
		<description>Timers Test SIP servlet</description>
		<servlet-class>
			org.mobicents.servlet.sip.testsuite.TimersSipServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<listener>
		<listener-class>
			org.mobicents.servlet.sip.testsuite.TimersSipServlet
		</listener-class>
	</listener>
</sip-app>
