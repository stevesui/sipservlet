	<display-name>MainHandler Test Sip Servlet</display-name>
	<description>MainHandler Test Sip Servlet</description>

	<servlet-selection>	
		<main-servlet>
			MainHandlerSipServlet
		</main-servlet>
	</servlet-selection>

	<servlet>
		<servlet-name>MainHandlerSipServlet</servlet-name>
		<display-name>MainHandlerSipServlet</display-name>
		<description>MainHandler Test SIP servlet</description>
