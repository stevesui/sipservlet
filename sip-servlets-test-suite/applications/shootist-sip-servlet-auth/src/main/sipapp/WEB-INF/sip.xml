<?xml version="1.0" encoding="UTF-8"?>

<sip-app>
	<app-name>org.mobicents.servlet.sip.testsuite.ShootistAuthApplication</app-name>
	<display-name>Shootist Sip Servlet</display-name>
	<description>Shootist Sip Servlet</description>
	
	<servlet-selection>
		<main-servlet>
			ShootistSipServlet
		</main-servlet>
	</servlet-selection>

	<servlet>
		<servlet-name>ShootistSipServlet</servlet-name>
		<display-name>ShootistSipServlet</display-name>
		<description>Shootist SIP servlet</description>
		<servlet-class>
			org.mobicents.servlet.sip.testsuite.ShootistSipServletAuth
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<listener>
		<listener-class>
			org.mobicents.servlet.sip.testsuite.ShootistSipServletAuth
		</listener-class>
	</listener>
</sip-app>