<?xml version="1.0" encoding="UTF-8"?>

<sip-app>
	<app-name>org.mobicents.servlet.sip.testsuite.ProxyApplication</app-name>
	<display-name>Proxy Sip Servlet</display-name>
	<description>Proxy Sip Servlet</description>

	<servlet-selection>	
		<main-servlet>
			ProxySipServlet
		</main-servlet>
	</servlet-selection>

	<servlet>
		<servlet-name>ProxySipServlet</servlet-name>
		<display-name>ProxySipServlet</display-name>
		<description>Proxy SIP servlet</description>
		<servlet-class>
			org.mobicents.servlet.sip.testsuite.ProxySipServlet
		</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<listener>
		<listener-class>
			org.mobicents.servlet.sip.testsuite.ProxySipServlet
		</listener-class>
	</listener>
</sip-app>